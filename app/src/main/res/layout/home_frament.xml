<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center">

    <TextView
        android:id="@+id/textViewIDNumber"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewPhone"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="35dp"
        android:layout_toEndOf="@+id/textView5"
        android:text="ID Number"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textViewPhone"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewAmazina"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="34dp"
        android:layout_toEndOf="@+id/textView4"
        android:text="Phone"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textViewAmazina"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/wel"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="93dp"
        android:layout_marginEnd="30dp"
        android:layout_toEndOf="@+id/textView2"
        android:text="amazina"
        android:textSize="24sp" />

    <TextView

        android:id="@+id/wel"
        android:layout_width="249dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="86dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="76dp"
        android:gravity="center"
        android:text="UMWIRONDORO"
        android:textColor="@color/w2"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/wel"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="90dp"
        android:text="Amazina:"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="40dp"
        android:text="Telephone:"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="40dp"
        android:text="ID No"
        android:textSize="24sp" />



</RelativeLayout>